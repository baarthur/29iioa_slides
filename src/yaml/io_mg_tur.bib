@article{guilhoto2005,
  title = {{Estima\c{c}\~ao da matriz insumo-produto a partir de dados preliminares das contas nacionais}},
  author = {Guilhoto, Joaquim J. M. and Sesso Filho, Umberto A.},
  year = {2005},
  journal = {Economia Aplicada},
  volume = {9},
  pages = {23},
  abstract = {The goal of this study is to present and to analyze a methodology to estimate Input-Output Matrices using preliminary data from the Brazilian National Accounts. This methodology is tested for the years of 1994 and 1996, where the results obtained from the input-output matrices (definitive version) available from IBGE are compared with the matrices estimated using the proposed methodology. The results analyzed refer to: a) type I production multipliers; b) Rasmussen-Hirschman backward and forward linkages; and c) Pure backward and forward linkages. Based on statistical methods (correlation indexes), the results show that the estimated matrices are similar to the ones released by IBGE. As so, this methodology can be used to estimate Brazilian national input-output matrices for the time periods where there are only preliminary data, and the economic structural analysis made with the estimated matrices are valid for the years analyzed.},
  langid = {portuguese},
  file = {C\:\\Users\\arthu\\Zotero\\storage\\E23XJAYA\\Guilhoto e Filho - 2005 - ESTIMAÇÃO DA MATRIZ INSUMO-PRODUTO A PARTIR DE DAD.pdf}
}

@article{guilhoto2010,
  title = {Estima\c{c}\~ao Da Matriz Insumo-Produto Utilizando Dados Preliminares Das Contas Nacionais: Aplica\c{c}\~ao e An\'alise de Indicadores Econ\^omicos Para o {{Brasil}} Em 2005},
  shorttitle = {Estima\c{c}\~ao Da Matriz Insumo-Produto Utilizando Dados Preliminares Das Contas Nacionais},
  author = {Guilhoto, Joaquim Jos{\'e} Martins and Sesso Filho, Umberto Antonio},
  year = {2010},
  month = dec,
  journal = {Revista Economia \& Tecnologia},
  volume = {6},
  number = {4},
  issn = {2238-1988, 2238-4715},
  doi = {10.5380/ret.v6i4.26912},
  abstract = {O presente estudo tem como objetivo avaliar a metodologia de estima\c{c}\~ao das matrizes de insumo-produto, a pre\c{c}os b\'asicos, a partir dos dados preliminares das Contas Nacionais do Brasil. A mesma \'e testada para o ano de 2005, sendo que os resultados obtidos a partir~da matriz de insumo-produto (vers\~ao definitiva) disponibilizada pelo IBGE e a matriz estimada~pela metodologia proposta s\~ao comparados. Os resultados analisados consistem dos multiplicadores de emprego e produ\c{c}\~ao e os \'indices de liga\c{c}\~ao intersetoriais de Rasmussen-Hirschman,~indicadores econ\^omicos baseados na teoria de insumo-produto. Conclui-se que as s\'eries de~indicadores econ\^omicos da matriz estimada e da disponibilizada pelo IBGE n\~ao s\~ao diferentes,~baseando-se em an\'alise estat\'istica (\'indices de correla\c{c}\~ao). Portanto, a metodologia proposta~pode ser utilizada para a estima\c{c}\~ao de matrizes de insumo-produto nacionais para per\'iodos em~que existem somente dados preliminares e as an\'alises estruturais da economia realizadas com~as matrizes estimadas s\~ao v\'alidas para o ano analisado.},
  file = {C\:\\Users\\arthu\\Zotero\\storage\\8K58N9TI\\Guilhoto e Sesso Filho - 2010 - Estimação da matriz insumo-produto utilizando dado.pdf}
}

