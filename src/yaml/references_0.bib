  shorttitle = {Estima\c{c}\~ao Da Matriz Insumo-Produto Utilizando Dados Preliminares Das Contas Nacionais},
  author = {Guilhoto, Joaquim Jos{\'e} Martins and Sesso Filho, Umberto Antonio},
