@phdthesis{tyszler2006,
  type = {{Disserta\c{c}\~ao}},
  title = {{Econometria espacial: discutindo medidas para a matriz de pondera\c{c}\~ao espacial}},
  author = {Tyszler, Marcelo},
  year = {2006},
  address = {{S\~ao Paulo}},
  urldate = {2021-10-29},
  abstract = {Com o crescente uso das teorias e m\'etodos vindos das \'areas de economia regional e urbana, economia e econometria espacial na fronteira entre as \'areas de economia e pol\'iticas p\'ublicas, estudar esse arcabou\c{c}o te\'orico e metodol\'ogico torna-se relevante no campo da an\'alise de pol\'iticas p\'ublicas. O objetivo geral dessa disserta\c{c}\~ao \'e discutir de modo cr\'itico aspectos da literatura de econometria espacial e sua aplicabilidade, com especial foco na compara\c{c}\~ao entre os diferentes modelos e matrizes de pondera\c{c}\~ao espacial. Esse estudo procura analisar diferentes formas de medidas que permitem a constru\c{c}\~ao de uma matriz de pondera\c{c}\~ao espacial, bem como testar por meio de simula\c{c}\~ao experimental e em uma aplica\c{c}\~ao emp\'irica a robustez de algumas dessas matrizes e de diferentes modelos. Dadas as indica\c{c}\~oes da literatura e as evid\^encias mostradas nessa disserta\c{c}\~ao, a estrat\'egia mais confi\'avel para a busca de um modelo parece ser utilizar as estimativas dos modelos, em conjunto com as estat\'isticas de Moran, para identificar presen\c{c}a de algum tipo de correla\c{c}\~ao espacial, e do teste LM da vari\'avel dependente, para indicar esse tipo de correla\c{c}\~ao especificamente. Observa-se que, de modo geral, n\~ao h\'a muita diferen\c{c}a em termos de esperan\c{c}a dos par\^ametros n\~ao espaciais, quando comparadas a estima\c{c}\~oes tradicionais com as que utilizam as t\'ecnicas espaciais. Com isso, a indica\c{c}\~ao que se pode fazer \'e que o uso das ferramentas de econometria espacial deve ser aplicado quando o interesse da an\'alise \'e justamente na estima\c{c}\~ao dos par\^ametros de correla\c{c}\~ao espacial. Comparando o uso da matriz de contig\"uidade no lugar da matriz de dist\^ancias, \'e poss\'ivel notar que a matriz de contig\"uidade n\~ao \'e, de forma geral, uma boa substitui\c{c}\~ao para a matriz de dist\^ancias. Ela capta parcialmente o efeito de rho e lambda, al\'em de captar erroneamente lambda, quando este inexiste.},
  langid = {brazilian},
  school = {Escola de Administra\c{c}\~ao de Empresas de S\~ao Paulo}
}

