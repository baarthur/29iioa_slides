@phdthesis{jardim2016,
  type = {{Trabalho de Conclus\~ao de Curso}},
  title = {{Desenvolvimento Urbano e Econ\^omico: uma conex\~ao entre pr\'aticas de planejamento urbano, estrutura espacial urbana e o desenvolvimento econ\^omico}},
